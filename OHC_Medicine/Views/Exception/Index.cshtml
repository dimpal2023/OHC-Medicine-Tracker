@using OHCModel.Model

@model ExceptionModels
@{
    ViewBag.Title = "Exception";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper">
    @*<div class="page-header">
            <h3 class="page-title">Employee Cum Patient List</h3>
            <div class="d-flex">
                <a href="@Url.Action("Create", "Emp_Patient")" type="button" class="btn btn-sm ml-3 btn-success"> Add New Employee Cum Patient </a>
            </div>
        </div>*@
    <div class="row">
        <div class="col-lg-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Exception List</h4>
                    <div class="table-responsive">
                        <table class="table table-striped" id="myTable">
                            <thead>
                                <tr>
                                    <th>SR No.</th>
                                    <th>ExceptionType</th>
                                    <th>BaseType</th>
                                    <th>Title</th>
                                    <th>Message</th>
                                    <th>StackTrace</th>
                                    <th>HelpLink</th>
                                    <th>Class</th>
                                    <th>Method</th>
                                    <th>LoggedUserId</th>
                                    <th>CreatedDate</th>
                                </tr>
                            </thead>
                            <tbody>

                                @foreach (var data in Model.exceptionModels)
                                {
                                    <tr>
                                        <td>@data.Id</td>
                                        <td>@data.ExceptionType</td>
                                        <td>@data.BaseType</td>
                                        <td>@data.Title</td>
                                        <td>@data.Message</td>
                                        <td>@data.StackTrace</td>
                                        <td>@data.HelpLink</td>
                                        <td>@data.Class</td>
                                        <td>@data.Method</td>
                                        <td>@data.LoggedUserId</td>
                                        <td>@data.CreatedDate</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

